#include <stdio.h>

int main()
{
    int a=2;
    float b=2.2;
    
    
    printf("Sum of a and b is %f",a+b);

    return 0;
}



#include <stdio.h>

int main()
{
   int a,b;
   printf("Enter a\n");
   scanf("%d",&a);
   
   printf("Enter b\n");
   scanf("%d",&b);
    printf("Sum of a and b is %d",a+b);

    return 0;
}
